#include<stdio.h>
#include<stdlib.h>
int size=0;
int capacity=5;
int *a;

void removeAtIndex(int pos)
{
    if(pos<0||pos>=size)
    {
        printf("Invalid position");
        return;
    }
    for(int i=pos;i<size-1;i++)
    {
        a[i]=a[i+1];
    }
    size--;
}
int main()
{
    int data;
    a=(int *)malloc(sizeof(int)*5);
    
    for(int i=0;i<5;i++)
    {
        scanf("%d",&data);
        a[i]=data;
        size++;
    }
    
    int pos;
    printf("Enter the position to remove: ");
    scanf("%d",&pos);
    removeAtIndex(pos);
    
    for(int i=0;i<size;i++)
    {
        printf("%d",a[i]);
    }return 0;
}
